        <div class="navbar-left">
            <div id="boards-list-btn" class="navbar-btn">Boards</div>
            <div id="boards-list">
                <div class="boards-list-sep"><span class="boards-list-sep-name">Personal Boards</span></div>
                <ul class="board-entry-list">
                </ul>
            </div>
        </div>
        <div class="navbar-mid">
            <p id="logo">Prello</p>
        </div>
        <div class="navbar-right">
            <div id="create-board-btn" class="navbar-btn">+</div>
            <a href="/logout"><div id="logout-btn" class="navbar-btn">Log Out</div></a>
            <div id="create-board-menu">
                <label>Title</label>
                <input id="create-board-input" placeholder="Enter board name...">
                <div id="create-board-submit-btn" class="btn">Create</div>
            </div>
        </div>
    </div>
    <div id="personal-boards" class="board-collection">
        <p class="collection-name">Personal Boards</p>
        <div class="boards-container">
        </div>
    </div>
    <script src="javascripts/jquery-3.2.1.js"></script>
    <script src="javascripts/boards.js"></script>
<%- include('common/footer'); %>